mode 1
definition "box1=min(x+1,1-x,y-1,3-y,z+1,1-z);
			cy1=min(0.5^2-(z)^2-(y-2)^2,1+x,5-x);
			cy2=min(2^2-(x)^2-(z)^2,2+y,3-y);
			fusion1=max(cy1, box1);
			fusion2=min(cy2, -fusion1);
			return fusion2;"
bboxSize 20 20 20
bboxCenter 0 0 0
bboxResolution 300 300 300
diffuseColor 0 1 0
backgroundColor 0.1 0.8 0.9
axesColor 0.45 0.1 0.55
SpecularColor 0.4 0.1 0.3
Transparency 0
Shininess 1
Timecycle 1
Timerange 0 1
